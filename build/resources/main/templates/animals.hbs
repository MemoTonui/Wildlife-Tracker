{{#partial "title"}}
{{/partial}}
{{#partial "content"}}
<div class="container">
    <h2>UNENDANGERED SPECIES SIGHTINGS</h2> <hr>
    <table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Animal id</th>
      <th scope="col">Animal Name</th>
      <th scope="col">Zone Sighted</th>
      
    </tr>
  </thead>
  {{#if animals}}
  <tbody>
      {{#each animals}}
    <tr>
      <th scope="row">{{id}}</th>
      <td>{{animalname}}</td>
      <td>{{location}}</td>
      
    </tr>
{{/each}}
  </tbody>
  {{/if}}
    </table>

</div>

{{/partial}}

{{> layout.hbs}}